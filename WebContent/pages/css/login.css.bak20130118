	Font-weight: regular;
	color :white;
	}
	.go
	{
	height: 105px;
	width: 170px;
	position: absolute;
	left: 505px;
	top: 404px;
		/*border-style: solid;
	border-width: 1px;
	border-color: red;*/
	background:url('../images/go_button.png') ;
	background-size: 100% 100%  ;
	}
	.go input[type="submit"]{
		background: transparent;
		border: none;
		color: white;
		cursor: pointer;
		height: 90px;
		width: 150px;
		font-size: 1.7em;
	}
	.go_text
	{
	Font-Family :Helvetica;
	Font-weight :CE Bold COndensed;
	color :white;
    text-align:center;
	}
	
	.content_bottom
	{
	width: 100%;
	height: 150px;
	position: absolute;
	left: 58;
	bottom: -74px;
	background: url('../images/bottom_ bg.png')no-repeat;
	background-size: 100% 100%;
	}
	.bottom_img
	{
	height: 75px;
	width: 142px;
	position: relative;
	left: 39px;
	top: 17px;
	padding-top: 10px;
	background: url('../images/register_button.png');
	background-size: 100% 100%;
	padding-left: 25px;
	}
	.bottom_img_text
	{
	color: white;
	text-decoration: none;
	position: relative;
	top: 6px;
	font-size: 1.5em;
	}
	
	.do
	{
	height: 35px;
	width: 250px;
	position: relative;
	left: 355px;
	top: -50px;
	
	
	padding-top:7px;
	}
	.do_text
	{
	Font-Family :Arial;
	Font-size:24pt;
	Font-weight:regular;
	color :black;
	text-align: center;
	text-decoration:none;
	}

	/*radha:added the css for error msg*/	
	.error_msg{
	height: 35px;
	width: 94%;
	margin:auto;
	margin-bottom: 100px;
	
	}
	.text_error{
	width:100%;
	height:35px;
	margin:auto;
	color:red;
	text-align:center;
	Font-Family :Arial;
	Font-size:24pt;
	Font-weight:regular;
	}
}

