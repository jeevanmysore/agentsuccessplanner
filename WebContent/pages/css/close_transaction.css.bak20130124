@media only screen and (min-device-width: 320px) and (max-device-width: 480px) {
	html{ font-size: 62.5%; }
	body
	{
		height:100%;
		width:100%
	}
	.header
	{
		margin-bottom:5px;
		width:100%;
	}
	
	
	/* ---------- LOGO SUBTITLE----------*/
	.logo_subtitle
	{
	width:100%;
	height:50%;
	background:url('../images/back_bg.png')repeat-x ;
	background-size: 100% 100% ;
	}
	.back_img
	{
	height:30px;
	float:left;
	}
	.back
	{
	height:100%;
	width:100%;
	float: left;
	}
	.logo
	{
	width:155px;
	height:70px;
	margin-right:25%;
	float:right;
	}
	.logo_img
	{
	height:100%;
	width:100%;
	}

	.subtitle
	{
	clear:both;
	width:100%;
	margin:0 auto;
	}
	.logo_subtitle_img
	{
	width:100%;
	height:35px;
	margin :0 auto;
	}
	/* ---------- LOGO SUBTITLE1----------*/
	.logo_subtitle1
	{
	width:100%;
	height:50px;
	background:url('../images/sub_title-bg.png') repeat-x;
	background-size: 100% 100%;
	}
	.view
	{
	height: 50%;
	width: 60%;
	Font-Family: Verdana;
	Font-Weight: regular;
	Font-Size: 1.5em;
	Color: #a20101;
